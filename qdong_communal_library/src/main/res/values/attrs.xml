<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!-- 星级评分控件 -->
    <declare-styleable   name="StarEvaluateView" >
        <attr name="linesCount"            format="integer" />
        <attr name="starsCount" format="integer" />
        <attr name="defaultChosen" format="float" />
        <attr name="stepSize" format="float" />
        <attr name="checkable" format="boolean" />
        <attr name="isBig" format="boolean" />
    </declare-styleable>


    <!-- PagerTab xml配置属性 -->
    <declare-styleable name="PagerTab">
        <attr name="PagerTabdividerPadding" format="dimension"/>
        <attr name="dividerWidth" format="dimension"/>
        <attr name="dividerColor" format="color|reference"/>
        <attr name="indicatorHeight" format="dimension"/>
        <attr name="indicatorLeft" format="dimension"/>
        <attr name="indicatorColor" format="color|reference"/>
        <attr name="tabTextSize" format="dimension"/>
        <attr name="PagerTabtabPadding" format="dimension"/>
        <attr name="tabBackgroundResId" format="reference"/>
        <attr name="tabTextColorResId" format="integer"/>
        <attr name="tabTextStyleBold" format="boolean"/>
    </declare-styleable>

</resources>